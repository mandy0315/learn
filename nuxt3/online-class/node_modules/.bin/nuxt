#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mandy/Desktop/projects/learn/2024-nuxt3-class/node_modules/.pnpm/nuxt@3.6.5_@types+node@18.17.6_eslint@8.47.0_sass@1.69.7_typescript@5.1.6_vue-tsc@1.8.8/node_modules/nuxt/bin/node_modules:/Users/mandy/Desktop/projects/learn/2024-nuxt3-class/node_modules/.pnpm/nuxt@3.6.5_@types+node@18.17.6_eslint@8.47.0_sass@1.69.7_typescript@5.1.6_vue-tsc@1.8.8/node_modules/nuxt/node_modules:/Users/mandy/Desktop/projects/learn/2024-nuxt3-class/node_modules/.pnpm/nuxt@3.6.5_@types+node@18.17.6_eslint@8.47.0_sass@1.69.7_typescript@5.1.6_vue-tsc@1.8.8/node_modules:/Users/mandy/Desktop/projects/learn/2024-nuxt3-class/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mandy/Desktop/projects/learn/2024-nuxt3-class/node_modules/.pnpm/nuxt@3.6.5_@types+node@18.17.6_eslint@8.47.0_sass@1.69.7_typescript@5.1.6_vue-tsc@1.8.8/node_modules/nuxt/bin/node_modules:/Users/mandy/Desktop/projects/learn/2024-nuxt3-class/node_modules/.pnpm/nuxt@3.6.5_@types+node@18.17.6_eslint@8.47.0_sass@1.69.7_typescript@5.1.6_vue-tsc@1.8.8/node_modules/nuxt/node_modules:/Users/mandy/Desktop/projects/learn/2024-nuxt3-class/node_modules/.pnpm/nuxt@3.6.5_@types+node@18.17.6_eslint@8.47.0_sass@1.69.7_typescript@5.1.6_vue-tsc@1.8.8/node_modules:/Users/mandy/Desktop/projects/learn/2024-nuxt3-class/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
